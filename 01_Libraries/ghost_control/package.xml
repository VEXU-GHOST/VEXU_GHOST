<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>ghost_control</name>
  <version>0.0.0</version>
  <description>Provides C++ Libraries for Modeling and Control for VEX V5 Robots</description>
  <maintainer email="jessemaxxwilson@utexas.edu">maxx</maintainer>
  <license>GPLv3.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <build_depend>ament_cmake_gtest</build_depend>
  <build_depend>eigen3_cmake_module</build_depend>
  <build_depend>ament_index_cpp</build_depend>

  <build_depend>urdf</build_depend>
  <build_depend>ghost_common</build_depend>
  <build_depend>ghost_estimation</build_depend>
  <build_depend>urdfdom</build_depend>

  <build_export_depend>ament_cmake</build_export_depend>
  <build_export_depend>ament_index_cpp</build_export_depend>
  <build_export_depend>urdf</build_export_depend>
  <build_export_depend>ament_cmake_gtest</build_export_depend>
  <build_export_depend>eigen3_cmake_module</build_export_depend>
  <build_export_depend>ghost_common</build_export_depend>
  <build_export_depend>ghost_estimation</build_export_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
