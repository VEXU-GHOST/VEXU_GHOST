<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>ghost_sim</name>
  <version>0.0.0</version>
  <description>Provides plugins for simulating VEX V5 Robots with Gazebo</description>
  <maintainer email="jessemaxxwilson@utexas.edu">maxx</maintainer>
  <license>GPLv3.0</license>

  <build_depend>ament_cmake_gtest</build_depend>
  <buildtool_depend>ament_cmake</buildtool_depend>
  <build_export_depend>ament_cmake</build_export_depend>

  <depend>rclcpp</depend>
  <depend>std_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>tf2_msgs</depend>
  <depend>visualization_msgs</depend>
  <depend>gazebo_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>nav_msgs</depend>

  <depend>gazebo</depend>
  <depend>gazebo_dev</depend>
  <depend>gazebo_ros</depend>
  <depend>gazebo_plugins</depend>

  <depend>ghost_msgs</depend>
  <depend>ghost_control</depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <exec_depend>rviz2</exec_depend>
  <exec_depend>plotjuggler</exec_depend>
  <exec_depend>robot_localization</exec_depend>
  <exec_depend>ghost_ros</exec_depend>
  <exec_depend>teleop_twist_joy</exec_depend>
  <exec_depend>robot_state_publisher</exec_depend>
  <exec_depend>joy_linux</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
