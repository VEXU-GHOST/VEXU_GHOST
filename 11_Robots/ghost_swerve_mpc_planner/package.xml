<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>ghost_swerve_mpc_planner</name>
  <version>0.0.0</version>
  <description>Provides C++ Libraries for Model Predictive Control for Swerve</description>
  <maintainer email="jessemaxxwilson@utexas.edu">maxx</maintainer>
  <license>GPLv3.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <build_depend>ament_cmake_gtest</build_depend>
  <build_depend>eigen3_cmake_module</build_depend>
  <build_depend>urdfdom</build_depend>

  <depend>rclcpp</depend>
  <depend>ghost_msgs</depend>
  <depend>ghost_estimation</depend>
  <depend>ghost_control</depend>
  <depend>ghost_util</depend>
  <depend>ghost_ros_interfaces</depend>

  <build_export_depend>ament_cmake</build_export_depend>
  <build_export_depend>ament_cmake_gtest</build_export_depend>
  <build_export_depend>eigen3_cmake_module</build_export_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
